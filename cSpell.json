  "languageId": "typescript,javascript",
  "version": "0.1",
  "words": [
    "axios",
    "avenir",
    "grayscale",
    "uikit",
    "vuex",
    "mkdirp",
    "tsmerge",
    "githubusercontent",
    "streetsidesoftware",
    "vsmarketplacebadge",
    "visualstudio",
    "fontawesome",
